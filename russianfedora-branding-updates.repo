[russianfedora-branding-updates]
name=Russian Fedora for Fedora $releasever - Branding - Updates
#baseurl=http://mirror.yandex.ru/fedora/russianfedora/russianfedora/branding/fedora/updates/$releasever/$basearch
mirrorlist=http://mirrors.rfremix.ru/mirrorlist?repo=branding-fedora-updates-released-$releasever&arch=$basearch
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-russianfedora-branding-fedora

